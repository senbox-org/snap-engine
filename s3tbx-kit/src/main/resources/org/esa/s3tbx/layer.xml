<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Menu">
        <folder name="Optical">
            <attr name="position" intvalue="500"/>
            <folder name="Preprocessing">
                <attr name="position" intvalue="200"/>
            </folder>
            <folder name="Thematic Land Processing">
                <attr name="position" intvalue="300"/>
            </folder>
            <folder name="Thematic Water Processing">
                <attr name="position" intvalue="400"/>
            </folder>

        </folder>
    </folder>

    <!--
        SNAP autoupdate extensions
    -->
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="s3tbx_update_center.instance">
                <attr name="displayName" stringvalue="Sentinel-3 Toolbox Update Center"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate"
                      methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" urlvalue="http://step.esa.int/updatecenter/2.0/s3tbx/updates.xml.gz"/>
            </file>
        </folder>
    </folder>

</filesystem>